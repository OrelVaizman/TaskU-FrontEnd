lor() {\n      return `color: ${this.group.color};`\n    },\n    board() {\n      return this.$store.getters.board\n    }\n  },\n  methods: {\n    getTime(time) {\n      return moment(time).format('LL')\n    },\n    closeTaskDetails() {\n      this.isShown = false\n    }\n  },\n  created() {\n    this.isShown = this.isTaskDetailsShown\n  }\n}\n</script>\n"],"sourceRoot":"src/cmps/Trello"}]}