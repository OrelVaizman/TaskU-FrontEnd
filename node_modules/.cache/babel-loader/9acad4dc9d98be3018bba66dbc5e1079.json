(time) {\n      return moment(time).fromNow()\n    },\n    deleteActivity(activityId) {\n      this.$emit('deleteActivity', activityId)\n    },\n    setHoveredActivity(actId) {\n      this.isActHovering = true\n      this.hoveredActId = actId\n    }\n  },\n  components: {\n    Avatar\n  },\n  created() {}\n}\n</script>\n\n<style></style>\n"],"sourceRoot":"src/cmps"}]}