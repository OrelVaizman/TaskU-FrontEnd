is.newPost\n            post.byUser = this.loggedInUser\n            this.copiedPosts.unshift(post)\n            this.$emit('updatePosts', this.copiedPosts)\n            this.newPost = ''\n        },\n    },\n    components: {\n        Avatar,\n    },\n}\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/cmps"}]}