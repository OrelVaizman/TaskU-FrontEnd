s.loggedInUser) {\n                this.$router.push('/board/' + this.firstBoardId)\n            } else {\n                await this.$store.dispatch({\n                    type: 'login',\n                    userCred: 'guest',\n                })\n                await this.$store.dispatch({ type: 'loadBoards' })\n                this.$router.push('/board/' + this.firstBoardId)\n            }\n        },\n        onLogout() {\n            console.log('Logging out')\n            this.$store.dispatch({\n                type: 'logout',\n            })\n        },\n    },\n    created() {\n        if (this.loggedInUser) {\n            this.$store.dispatch({ type: 'loadBoards' })\n        }\n    },\n}\n</script>\n"]}]}