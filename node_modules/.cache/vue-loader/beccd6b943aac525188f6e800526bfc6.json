  itemClicked(itemFunction) {\n            itemFunction()\n            this.$emit('closePopup')\n        },\n    },\n    mounted() {\n        if (this.offsetY) {\n            let elpopUp = this.$refs.popup\n            elpopUp.style.left = '95%'\n            elpopUp.style.top = `${this.offsetY}px`\n        }\n    },\n}\n</script>\n"]}]}