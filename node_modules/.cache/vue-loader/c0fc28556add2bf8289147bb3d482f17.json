s flex\" v-if=\"isFilterShowen\">\n          <form @submit.prevent=\"updateFilter\" class=\"filter-status flex column justify-center\">\n            <h4>Status</h4>\n            <button type=\"submit\">Done</button>\n            <button type=\"submit\">Working on it</button>\n            <button type=\"submit\">Stuck</button>\n          </form>\n          <form class=\"filter-priority flex column justify-center\">\n            <h4>Priority</h4>\n            <button>Low</button>\n            <button>Medium</button>\n            <button>High</button>\n          </form>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nexport default {\n  name: 'board-filter',\n  props: {\n    creator: Object\n  },\n  data() {\n    return {\n      // filterBy: {status: },\n      isFilterShowen: false\n    }\n  },\n  methods: {\n    addGroup() {\n      this.$emit('addGroup')\n    },\n    toggleFilter() {\n      this.isFilterShowen = !this.isFilterShowen\n    },\n    updateFilter(ev){\n      console.log(ev.submitter.innerText);\n      // this.$store.dispatch({\n      //           type: \"setFilterBy\",\n      //           filterBy: ev.target.innerText,\n      //       });\n    }\n  },\n  created() {\n    console.log('creator:')\n  }\n}\n</script>\n"]}]}